{
  "name": "clappr-stats",
  "version": "0.1.3",
  "description": "clappr playback stats report plugin",
  "main": "dist/clappr-stats.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:leandromoreira/clappr-stats.git"
  },
  "scripts": {
    "test": "node_modules/.bin/karma start --single-run",
    "test_watch": "node_modules/.bin/karma start",
    "watch": "node_modules/.bin/webpack --progress --watch",
    "start": "node_modules/.bin/webpack-dev-server --host 0.0.0.0 --content-base public/ --output-public-path latest/ --hot",
    "build": "node_modules/.bin/webpack",
    "release": "node_modules/.bin/webpack && node_modules/.bin/webpack --progress --optimize-minimize --output-filename clappr-stats.min.js"
  },
  "browser": {
    "Clappr" : "clappr"
  },
  "author": "leandro.ribeiro.moreira@gmail.com",
  "license": "BSD-3-Clause",
  "dependencies": {
    "lodash.get": "^4.3.0"
  },
  "peerDependencies": {
    "clappr": "^0.2.66"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "clappr": "^0.2.66",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "sinon": "^2.3.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
